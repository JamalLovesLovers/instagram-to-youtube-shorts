{
    "manifest_version": 3,
    "name": "Instagram Reel Downloader",
    "version": "1.0",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://www.instagram.com/reels/*"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["http://127.0.0.1:5000/"]
  }
  